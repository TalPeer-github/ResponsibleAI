  {
   "cell_type": "code",
   "execution_count": 132,
   "id": "1a6114d7d3d0214c",
   "metadata": {
    "id": "1a6114d7d3d0214c",
    "ExecuteTime": {
     "end_time": "2024-08-17T13:01:07.532589200Z",
     "start_time": "2024-08-17T13:01:07.029659200Z"
