  {
   "cell_type": "code",
   "source": [
    "def cohens_d(group1, group2):\n",
    "    n1, n2 = len(group1), len(group2)\n",
    "    s1, s2 = np.var(group1, ddof=1), np.var(group2, ddof=1)\n",
    "    s = np.sqrt(((n1 - 1) * s1 + (n2 - 1) * s2) / (n1 + n2 - 2))\n",
    "    u1, u2 = np.mean(group1), np.mean(group2)\n",
    "    return (u1 - u2) / s\n",
    "\n",
    "\n",
    "d_value = cohens_d(test_df[test_df['Gender'] == 'M']['Predicted_Decision'],\n",
    "                   test_df[test_df['Gender'] == 'F']['Predicted_Decision'])"
   ],
   "metadata": {
    "id": "HdDir3dZekZK",
    "ExecuteTime": {
     "end_time": "2024-08-17T13:01:15.603381900Z",
     "start_time": "2024-08-17T13:01:15.487842700Z"
    }
   },
   "id": "HdDir3dZekZK",
   "execution_count": 163,
   "outputs": []
